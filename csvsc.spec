# -*- mode: python ; coding: utf-8 -*-

block_cipher = None

a = Analysis(
    ['csvsc.py'],
    pathex=[],
    binaries=[],
    datas=[],
    hiddenimports=[],
    hookspath=[],
    hooksconfig={},
    runtime_hooks=[],
    excludes=[],
    win_no_prefer_redirects=False,
    win_private_assemblies=False,
    cipher=block_cipher,
    noarchive=False,
)

# fnフォルダ内のファイルを除外
a.datas = [x for x in a.datas if not x[0].startswith('fn/')]
